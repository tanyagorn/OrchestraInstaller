<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<processing>
  <logfiledir>$INSTALL_PATH</logfiledir>

  <job name="Install configuration file">
    <os family="windows" />
    <executefile name="$INSTALL_PATH/Orchestra/Startscripts/Windows/orchestra_env.bat">
      <env>ORC_JAVA_HOME=${ENV[ORC_JAVA_HOME]}</env>
      <env>ORC_HOME=$INSTALL_PATH</env>
    </executefile>
  </job> 

  <job name="setup">
    <os family="windows" />
    <executeclass name="emds.epi.impl.security.HelloWorld">
        <arg>${USERNAME}</arg><arg>$INSTALL_PATH</arg>
    </executeclass>
  </job>
  
</processing>
