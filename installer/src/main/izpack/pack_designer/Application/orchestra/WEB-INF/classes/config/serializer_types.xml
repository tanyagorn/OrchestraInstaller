<!--
	out.internal out.csv out.xls out.txt out.fixed.text out.field.values out.hl7 out.edi out.hcm 
	in.internal in.ref in.csv in.xls in.txt in.hl7 in.edi in.hcm
 -->
		  
	<format name="INTERNAL">
	  <outbound preferred="out.internal">
		<alt>out.csv</alt><alt>out.xls</alt><alt>out.xml</alt><alt>out.txt</alt>
		<alt>out.fixed.text</alt><alt>out.field.values</alt>
	  </outbound>
	  <inbound preferred="in.internal">
		<alt>in.csv</alt><alt>in.xls</alt><alt>in.xml</alt><alt>in.ref</alt><alt>in.txt</alt>
		<alt>in.hl7</alt><alt>in.edi</alt><alt>in.hcm</alt>
	  </inbound>
	</format>

	<format name="XML">
	  <outbound preferred="out.xml">
		<alt>out.csv</alt><alt>out.xls</alt><alt>out.txt</alt><alt>out.fixed.text</alt>
	  </outbound>
	  <inbound preferred="in.xml">
		<alt>in.csv</alt><alt>in.xls</alt><alt>in.ref</alt><alt>in.txt</alt>
	  </inbound>
	</format>
	
	<format name="CSV">
	  <outbound preferred="out.csv">
		<alt>out.xls</alt><alt>out.xml</alt><alt>out.txt</alt><alt>out.fixed.text</alt>
	  </outbound>
	  <inbound preferred="in.csv">
		<alt>in.xls</alt><alt>in.xml</alt><alt>in.ref</alt><alt>in.txt</alt>
	  </inbound>
	</format>
		  
	<format name="HL7">
	  <outbound preferred="out.hl7">
		<alt>out.xml</alt><alt>out.fixed.text</alt>
	  </outbound>
	  <inbound preferred="in.hl7">
		<alt>in.xml</alt>
	  </inbound>
	</format>
		  
	<format name="EDI">
	  <outbound preferred="out.edi">
		<alt>out.xml</alt><alt>out.fixed.text</alt>
	  </outbound>
	  <inbound preferred="in.edi">
		<alt>in.xml</alt>
	  </inbound>
	</format>
		  
	<format name="HCM">
	  <outbound preferred="out.hcm">
		<alt>out.xml</alt><alt>out.csv</alt><alt>out.xls</alt>
	  </outbound>
	  <inbound preferred="in.hcm">
		<alt>in.xml</alt><alt>in.csv</alt><alt>in.xls</alt><alt>in.ref</alt>
	  </inbound>
	</format>
 	
	<format name="DICOM">
	  <outbound preferred="out.dicom">
		<alt>out.xml</alt>
	  </outbound>
	  <inbound preferred="in.dicom">
		<alt>in.xml</alt>
	  </inbound>
	</format>
	
	<format name="XDT">
	  <outbound preferred="out.xdt">
		<alt>out.xml</alt>
	  </outbound>
	  <inbound preferred="in.xdt">
		<alt>in.xml</alt>
	  </inbound>
	</format>	
 
</serializer_types>
